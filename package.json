{
  "name": "aronium-desktop-wrapper",
  "version": "1.0.0",
  "description": "An Analytical Wrapper for Aronium Free version",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icons/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . aronium-desktop-wrapper --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Aronium Wrapper App\"",
    "package-linux": "electron-packager . aronium-desktop-wrapper --overwrite --no-asar --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createInstaller.js"
  },
  "repository": "https://github.com/pavanvattikala/aronium-desktop-wrapper",
  "keywords": [
    "Electron",
    "php",
    "Aronium",
    "Analytics"
  ],
  "author": "Pavan Vattikala",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.0",
    "electron-packager": "^15.2.0",
    "electron-winstaller": "^5.2.2"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "ip": "^2.0.1",
    "php-server-manager": "^1.3.3"
  }
}
